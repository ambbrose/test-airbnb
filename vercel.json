{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      },
      {
        "src": ".next/**",
        "use": "@vercel/static"
      },
      {
        "src": "prisma/schema.prisma",
        "use": "prisma"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ],
    "env": {
      "PRISMA_GENERATE_BEFORE_BUILD": "true"
    }
  }
  